{
  "name": "nexusone",
  "version": "3.0.0",
  "description": "AI-Powered Customer Engagement Platform for Monbase-OpenLive Ecosystem",
  "main": "server.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "dev": "NODE_ENV=development nodemon --experimental-json-modules server.js",
    "start": "NODE_ENV=production node --experimental-json-modules server.js",
    "prod": "NODE_ENV=production node --experimental-json-modules server.js",
    "lint": "eslint . --ext .js",
    "format": "prettier --write .",
    "security-check": "npm audit",
    "docker-build": "docker build -t nexusone .",
    "docker-run": "docker run -p 3000:3000 --env-file .env nexusone"
  },
  "author": "NexusOne Engineering Team <engineering@nexusone.ai>",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/nexusone/chatbot-core.git"
  },
  "bugs": {
    "url": "https://github.com/nexusone/chatbot-core/issues"
  },
  "keywords": [
    "ai",
    "chatbot",
    "nlp",
    "customer-analytics",
    "enterprise-solution"
  ],
  "dependencies": {
    "@google-cloud/logging": "^10.3.0",
    "axios": "^1.5.0",
    "chalk": "^5.3.0",
    "compromise": "^14.11.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "google-spreadsheet": "^3.3.0",
    "helmet": "^8.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "natural": "^5.2.4",
    "sentiment": "^5.0.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/preset-env": "^7.22.10",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.6.2",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.20.1",
    "npm": ">=9.8.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/",
      "/config/"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/tests/**"
    ],
    "testTimeout": 30000
  },
  "type": "module",
  "volta": {
    "node": "18.20.1",
    "npm": "9.8.1"
  }
}
